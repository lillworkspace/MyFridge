<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="watermelons.myfridge">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/watermelon_logo"
        android:label="UseByApp"
        android:roundIcon="@mipmap/watermelon_logo_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".GroceryActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".PantryActivity"></activity>

    </application>


</manifest>